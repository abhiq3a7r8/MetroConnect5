{
  "expo": {
    "name": "MetroConnect5",
    "slug": "MetroConnect5",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-barcode-scanner"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "myapp",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5f8b16c8-d6c0-47fb-bc79-1f7a4f13f218"
      }
    },
    "android": {
      "package": "com.abhi_q3a7r8.MetroConnect5",
      "permissions": [
        "CAMERA"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.abhi-q3a7r8.MetroConnect5",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to the camera to scan QR codes and barcodes."
      }
    }
  }
}